{
  "name": "@yourtrading-ai/beet",
  "version": "0.1.1",
  "description": "Strict borsh compatible de/serializer",
  "main": "dist/src/beet.js",
  "types": "dist/src/beet.d.ts",
  "exports": {
    ".": "./dist/src/beet.js"
  },
  "scripts": {
    "check:publish-ready": "yarn build",
    "preversion": "yarn check:publish-ready",
    "postversion": "git add package.json && git commit -m \\\\\\\"chore: update $npm_package_name to v$npm_package_version\\\\\\\" && git tag $npm_package_name@$npm_package_version",
    "prepublishOnly": "yarn check:publish-ready",
    "postpublish": "git push origin && git push origin --tags",
    "build": "node_modules/.bin/rimraf dist && tsc -p tsconfig.json",
    "pretest": "yarn build",
    "test:browser": "node_modules/.bin/airtap ./test/{unit,compat}/*.ts",
    "bsamples": "cd ../tools/bsamples && cargo run",
    "lint": "node_modules/.bin/prettier -c src",
    "lint:fix": "node_modules/.bin/prettier --write src",
    "doc": "node_modules/.bin/rimraf ../docs/beet && typedoc",
    "doctoc": "node_modules/.bin/doctoc README.md"
  },
  "repository": "git@github.com:yourtrading-ai/beet.git",
  "author": "Thorsten Lorenz <thlorenz@gmx.de>, Mike Hukiewitz <mike.hukiewitz@robotter.ai>",
  "license": "Apache-2.0",
  "directories": {
    "dist": "dist/src"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ansicolors": "^0.3.2",
    "bn.js": "^5.2.0",
    "debug": "^4.3.3"
  },
  "devDependencies": {
    "@types/ansicolors": "^0.0.33",
    "@types/bn.js": "^5.1.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^14.18.0",
    "@types/tape": "^4.13.2",
    "airtap": "^4.0.4",
    "airtap-system": "^0.1.0",
    "browserify": "^17.0.0",
    "doctoc": "^2.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "spok": "^1.4.3",
    "supports-color": "^8.1.1",
    "tape": "^5.3.2",
    "tsify": "^5.0.4",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  }
}
